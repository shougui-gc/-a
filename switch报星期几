int main()//switch
{
	int day = 0;
	scanf("%d", &day);
	switch (day)//整形表达式
	{
	case 1 :
	case 2:	
	case 3:	
	case 4:	
	case 5:                         //无break阻止，这些case会一直往下划，执行完1继续搞2，接着3，4，直到遇到5
		printf("工作日\n");
		break;
	case 6:	
	case 7:
		printf("睡懒觉\n");
		break;
	default:                         //default爱放哪里放哪里，无顺序可言
		printf("输入错误\n";);
		break;
	}
}
